<A NAME="contact"></A>
<div class="contact">
	<h1>Envie uma mensagem:</h1>
	<div class="form-contact">
		<%= form_tag(send_email_path method: :post) do %>
			
			<h3>Digite o seu nome:</h3>
				 <input type="text" name="first_name"><br>

			<h3>Digite o seu email para entrarmos em contato:</h3>
				 <%= email_field_tag :email %><br>
			
			<h3>Se você quiser, digite seu telefone (Com DDD):</h3>
				<input type="text" name="tel"><br>
			
			<h3>Digite o assunto da sua mensagem:</h3>
				 <input type="text" name="subject"><br>

			<h3>Escreva a sua mensagem:</h3>
				<%= text_area_tag :comment %><br><br><br>

				<%= submit_tag "Enviar Mensagem", id: "btn-send-email" %>
		<% end %>
	</div>
</div>

